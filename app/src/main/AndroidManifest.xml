<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dam.oldbooks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ob_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ob_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OldBooks">
        <activity android:name=".OBMain"
            android:parentActivityName=".OBLogin"></activity>
        <activity android:name=".OBLogin"></activity>
        <activity android:name=".OBSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>